{#
/**
 * @file apd_membership.html.twig
 * Default theme implementation to present APD membership data.
 *
 * This template is used when viewing APD membership pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_apd_membership()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('apd_membership') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
